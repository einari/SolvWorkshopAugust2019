{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (Fundamentals)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/Fundamentals/bin/Debug/netcoreapp2.2/Fundamentals.dll",
            "args": [],
            "cwd": "${workspaceRoot}/Fundamentals",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
